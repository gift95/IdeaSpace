package com.yunhe.day08;

import java.util.Arrays;

public class StudentsBiz {
	int i;
	String[] names = new String[4];

	public void addName(String name) {
names[i] = name;

	}
	// 增加学生姓名

	public void showNames() { // 显示全部学生姓名
		for (int i = 0; i < 5; i++) {
			System.out.println(names[i]);
		}
	}
}
